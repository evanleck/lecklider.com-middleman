<div class='container'>
  <h1>Articles tagged '<%= tagname %>'</h1>

  <ul class='posts' id='post-list'>
    <% articles.each do |article| %>
      <li>
        <%= link_to article.title, article %>
        <span class='date'>
          <%= article.date.strftime('%b %d, %Y') %>
        </span>
      </li>
    <% end %>
  </ul>
</div>

